<!DOCTYPE html>
<html>
  <head>
    <title>RealestateApp</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
   
    <meta name="description" content="SEO friendly">
    <meta name="keywords" content="SEO friendly">
  
    <%= stylesheet_link_tag    'theme', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  </head>
  <body>
    <%= render 'shared/notices' %>
    <%= render "shared/home_nav" %>
    <div class="mt-8">
        <%= yield %> 
    </div>
    <div class="mt-3">
      <%= render "shared/home_footer" %>
    </div>
  </body>
</html>
