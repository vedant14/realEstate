<div class="container">
	<div class="row">
		<%= render "propertycontrols" %>
	</div>
	<%= render "show_images" %>
	<div class="row">
		<div class="col-md-8 vl">
			<%= render "show_property" %>
		</div>
		<div class="col-md-4 side-cont">
			<% if user_or_admin(@property) %>
				<%= render "offers" %>
			<% else %>
				<%= render "new_offer" %>
			<% end %>
		</div>
	</div>
</div>
